import java.util.Scanner;

public class Ejercicio10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		//Realiza un programa que sume los 100 números siguientes a un número entero y
		//positivo introducido por teclado. Se debe comprobar que el dato introducido es correcto (que
		//es un número positivo)
		
		Scanner teclado = new Scanner(System.in);
		System.out.println("Ingrese un número: ");
		int n = teclado.nextInt();
		

    	int i = 1;
		int suma = 0;
		
		if(n >= 0)
		{
            while( i <= 100) {
            	
				suma += n + i;
				System.out.println(n + i);
				i++;
			}
            
        	System.out.println(suma);
	    }else {
	    	System.out.println("Debe ingresar un número entero y positivo");
	    }
	
	}

}
